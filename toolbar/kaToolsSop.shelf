# $arg5 - which input to wire to
# $arg6 - output node to wire to
# $arg7 - the type of this node
# $arg8 - the node is an indirect input
# $arg9 - index of output from $arg6

\set noalias = 1
set saved_path = `execute("oppwf")`
opcf $arg1

# Node $_obj_packed_disk_instance_packed_prims (Sop/attribwrangle)
set _obj_packed_disk_instance_packed_prims = `run("opadd -e -n -v attribwrangle instance_packed_prims")`
oplocate -x `$arg2 + 0` -y `$arg3 + 0` $_obj_packed_disk_instance_packed_prims
opspareds '    group {         name    "folder1"         label   "Code"          parm {             name    "group"             baseparm             label   "Group"             export  none             bindselector points "Modify Points"                 "Select the points to affect and press Enter to complete."                 0 1 0xffffffff 0 grouptype 0         }         parm {             name    "grouptype"             baseparm             label   "Group Type"             export  none         }         parm {             name    "class"             baseparm             label   "Run Over"             export  none         }         parm {             name    "vex_numcount"             baseparm             label   "Number Count"             export  none         }         parm {             name    "vex_threadjobsize"             baseparm             label   "Thread Job Size"             export  none         }         parm {             name    "snippet"             baseparm             label   "VEXpression"             export  all         }         parm {             name    "exportlist"             baseparm             label   "Attributes to Create"             export  none         }         parm {             name    "vex_strict"             baseparm             label   "Enforce Prototypes"             export  none         }     }      group {         name    "folder1_1"         label   "Bindings"          parm {             name    "autobind"             baseparm             label   "Autobind by Name"             export  none         }         multiparm {             name    "bindings"             label    "Number of Bindings"             baseparm             default 0             parmtag { "autoscope" "0000000000000000" }             parmtag { "multistartoffset" "1" }              parm {                 name    "bindname#"                 baseparm                 label   "Attribute Name"                 export  none             }             parm {                 name    "bindparm#"                 baseparm                 label   "VEX Parameter"                 export  none             }         }          parm {             name    "groupautobind"             baseparm             label   "Autobind Groups by Name"             export  none         }         multiparm {             name    "groupbindings"             label    "Group Bindings"             baseparm             default 0             parmtag { "autoscope" "0000000000000000" }             parmtag { "multistartoffset" "1" }              parm {                 name    "bindgroupname#"                 baseparm                 label   "Group Name"                 export  none             }             parm {                 name    "bindgroupparm#"                 baseparm                 label   "VEX Parameter"                 export  none             }         }          parm {             name    "vex_cwdpath"             baseparm             label   "Evaluation Node Path"             export  none         }         parm {             name    "vex_outputmask"             baseparm             label   "Export Parameters"             export  none         }         parm {             name    "vex_updatenmls"             baseparm             label   "Update Normals If Displaced"             export  none         }         parm {             name    "vex_matchattrib"             baseparm             label   "Attribute to Match"             export  none         }         parm {             name    "vex_inplace"             baseparm             label   "Compute Results In Place"             export  none         }         parm {             name    "vex_selectiongroup"             baseparm             label   "Output Selection Group"             export  none         }     }      parm {         name    "pathAtr"         label   "Pathatr"         type    string         default { "" }     }     parm {         name    "viewportlod"         label   "Display As"         type    ordinal         default { "2" }         menu {             "full"      "Full Geometry"             "points"    "Point Cloud"             "box"       "Bounding Box"             "centroid"  "Centroid"             "hidden"    "Hidden"         }     } ' $_obj_packed_disk_instance_packed_prims
opparm $_obj_packed_disk_instance_packed_prims  bindings ( 0 ) groupbindings ( 0 )
opparm $_obj_packed_disk_instance_packed_prims snippet ( 'int primnum = addprim(geoself(), "PackedDisk", @ptnum);\n\nsetprimintrinsic(geoself(), "viewportlod", primnum, chs("viewportlod"), "set");\n\nsetprimintrinsic(geoself(), "unexpandedfilename", primnum, s@`chs("pathAtr")`, "set");\n\nstring atrs[] = detailintrinsic(geoself(), "pointattributes");\n    int find[] = find(atrs, "orient");\n    if(len(find) >0)\n    {\n        matrix3 mat = qconvert(vector4(point(0,"orient", @ptnum)));\n        setprimintrinsic(geoself(), "transform", primnum, mat, "set");\n    }' ) pathAtr ( instancepath )
opcolor -c 1 0.80000001192092896 0 $_obj_packed_disk_instance_packed_prims
opset -d on -r on -h off -f off -y off -t off -l off -s off -u off -F off -c on -e on -b off $_obj_packed_disk_instance_packed_prims
opexprlanguage -s hscript $_obj_packed_disk_instance_packed_prims
opuserdata -n '___Version___' -v '' $_obj_packed_disk_instance_packed_prims
opset -p on $_obj_packed_disk_instance_packed_prims

opcf $arg1
opwire -n $_obj_packed_disk_rand_orient -0 $_obj_packed_disk_instance_packed_prims

set oidx = 0
if ($argc >= 9 && "$arg9" != "") then
    set oidx = $arg9
endif

if ($argc >= 5 && "$arg4" != "") then
    set output = $_obj_packed_disk_instance_packed_prims
    opwire -n $output -$arg5 $arg4
endif
if ($argc >= 6 && "$arg6" != "") then
    set input = $_obj_packed_disk_instance_packed_prims
    if ($arg8) then
        opwire -n -i $arg6 -0 $input
    else
        opwire -n -o $oidx $arg6 -0 $input
    endif
endif
opcf $saved_path
'''
hou.hscript(h_preamble + h_extra_args + h_cmd)
]]></script>
